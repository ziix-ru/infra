services:
  whoami:
    image: traefik/whoami
    restart: unless-stopped
    labels:
      traefik.enable: "true"
      traefik.http.routers.whoami.rule: "PathPrefix(`/whoami`)"
      traefik.http.routers.whoami.entrypoints: "web"
      traefik.docker.network: "web"
    networks:
      - web

networks:
  web:
    external: true
